Host github.com
        User git
        Hostname github.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-github

Host gitlab.com
        User git
        Hostname gitlab.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-gitlab

Host pallaire.com
        User pallaire
        Hostname pallaire.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-hostgator
	HostKeyAlgorithms +ssh-rsa
	PubkeyAcceptedKeyTypes +ssh-rsa
