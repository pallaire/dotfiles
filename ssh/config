        User git
        Hostname bitbucket.org
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-bitbucket

Host gitlab.com
        User git
        Hostname gitlab.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-gitlab

Host pallaire.com
        User pallaire
        Hostname pallaire.com
        PreferredAuthentications publickey
        IdentityFile ~/.ssh/pallaire-hostgator
